from sqlalchemy import Column, String
from sqlalchemy.ext.declarative import declarative_base

Base = declarative_base()

class Concert(Base):
    __tablename__ = "concert"
    
    concert_id = Column(String, primary_key=True)
    name = Column(String)
    image = Column(String)
    description = Column(String)
    seat_count = Column(String)
    date = Column(String)
    place = Column(String)
    price = Column(String)
